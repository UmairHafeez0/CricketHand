<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ShapeAppearance.TopRounded" parent="ShapeAppearance.Material3.Corner.None">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">20dp</item>
        <item name="cornerSizeTopRight">20dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>
    <style name="ShapeAppearance.Circle" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>



    <style name="Theme.MyApplication.AppBarOverlay" parent="ThemeOverlay.MaterialComponents.Dark.ActionBar">
        <item name="android:textColorPrimary">@android:color/white</item>
    </style>

    <style name="Theme.MyApplication.PopupOverlay" parent="ThemeOverlay.MaterialComponents.Light">
        <!-- Popup menu style -->
    </style>
    <style name="NoButtonTint" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorButtonNormal">@android:color/transparent</item>
    </style>
    <style name="RoundedCornerImage" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">6dp</item>
    </style>

    <style name="SwipeRefreshLayoutStyle" parent="Theme.AppCompat.Light">
        <item name="android:clipToPadding">false</item>
        <item name="android:fitsSystemWindows">true</item>
    </style>
</resources>